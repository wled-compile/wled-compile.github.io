	"settings":[
		{
			"name" : "Include Debug flags",
			"sources" : [0, 1, 2],
			"type" : "checkbox",
			"build_flags" : "${common.debug_flags}",
			"lib_deps" : ""
		},
		{
			"name" : "Include sound reactive debug flags",
